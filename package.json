{
  "name": "styled-react-app",
  "version": "1.0.0",
  "description": "A universal react app with styled-components and redux support",
  "main": "build/server/index.js",
  "scripts": {
    "build:server": "babel ./src --out-dir build",
    "build:client": "BABEL_ENV=webpack webpack",
    "build": "npm run build:server && npm run build:client",
    "clean": "rimraf build && rimraf build-static",
    "start": "NODE_ENV=production node ./build/server/index.js",
    "start-dev": "babel-node src/server",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:js": "eslint src/**/*.js && eslint src/**/*.jsx",
    "lint:css": "stylelint \"./src/**/*.jsx\"",
    "lint": "npm run lint:js && npm run lint:css",
    "prettier:verify": "prettier --check .",
    "pretest": "jest --clearCache",
    "test": "jest",
    "ci:test": "jest --coverage",
    "storybook": "start-storybook -p 3001",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruv-m-patel/styled-react-app.git"
  },
  "author": "Dhruv Patel<dhruv.codelab@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/dhruv-m-patel/styled-react-app#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@loadable/component": "^5.14.1",
    "@loadable/server": "^5.14.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "confit": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-enrouten": "^1.3.0",
    "fast-deep-equal": "^3.1.3",
    "fetch-everywhere": "^1.0.5",
    "meddleware": "^3.0.4",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet-async": "^1.0.7",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.1",
    "shortstop-handlers": "^1.0.1",
    "shortstop-regex": "0.0.1",
    "store": "^2.0.12",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.0",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^4.2.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
