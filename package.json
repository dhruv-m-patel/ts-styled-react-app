    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "confit": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-enrouten": "^1.3.0",
    "fast-deep-equal": "^3.1.3",
    "fetch-everywhere": "^1.0.5",
    "meddleware": "^3.0.4",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
