{
  "name": "ts-styled-react-app",
  "version": "1.0.0",
  "description": "A universal react app with styled-components and redux support",
  "main": "build/server/index.js",
  "scripts": {
    "build:server": "babel ./src --out-dir build --extensions \".ts,.tsx\" --ignore \"**/*.test.tsx, **/*.stories.js\"",
    "build:client": "BABEL_ENV=webpack webpack",
    "build": "yarn build:server && yarn build:client",
    "clean": "rimraf build && rimraf build-static",
    "start": "NODE_ENV=production node ./build/server/index.js",
    "dev": "babel-node --extensions \".ts,.tsx\" src/server/index.ts",
    "start:watch": "yarn clean && yarn build && nodemon",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "yarn lint:ts && yarn lint:css",
    "prettier:format": "prettier --write .",
    "prettier:verify": "prettier --check .",
    "pretest": "jest --clearCache",
    "test": "jest",
    "ci:test": "jest --coverage",
    "storybook": "start-storybook -p 3001",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruv-m-patel/ts-styled-react-app.git"
  },
  "author": "Dhruv Patel<dhruv.codelab@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/dhruv-m-patel/ts-styled-react-app#readme",
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@dhruv-m-patel/web-app": "^1.8.0",
    "@loadable/component": "^5.16.3",
    "@loadable/server": "^5.16.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "confit": "^3.0.0",
    "core-js": "^3.36.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.1",
    "redux-api-middleware": "^3.2.1",
    "shortstop-handlers": "^1.1.1",
    "shortstop-regex": "0.0.1",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/plugin-transform-typescript": "^7.23.6",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@dhruv-m-patel/eslint-config-react": "^1.1.1",
    "@loadable/babel-plugin": "^5.16.1",
    "@loadable/webpack-plugin": "^5.15.2",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/confit": "^2.0.3",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-api-middleware": "^3.2.6",
    "@types/store": "^2.0.5",
    "babel-loader": "^8.3.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.57.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^15.2.2",
    "nodemon": "^2.0.22",
    "prettier": "3.2.5",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "packageManager": "yarn@3.2.3"
}
