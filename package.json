{
  "name": "ts-styled-react-app",
  "version": "1.0.0",
  "description": "A universal react app with styled-components and redux support",
  "main": "build/server/index.js",
  "scripts": {
    "build:server": "babel ./src --out-dir build --extensions \".ts,.tsx\" --ignore \"src/**/*.stories.js\",\"src/**/*.test.tsx\"",
    "build:client": "BABEL_ENV=webpack webpack",
    "build": "yarn build:server && yarn build:client",
    "clean": "rimraf build && rimraf build-static",
    "start": "NODE_ENV=production node ./build/server/index.js",
    "start-dev": "babel-node --extensions \".ts,.tsx\" src/server/index.ts",
    "start:watch": "yarn clean && yarn build && nodemon",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "yarn lint:ts && yarn lint:css",
    "prettier:verify": "prettier --check .",
    "pretest": "jest --clearCache",
    "test": "jest",
    "ci:test": "jest --coverage",
    "storybook": "start-storybook -p 3001",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruv-m-patel/ts-styled-react-app.git"
  },
  "author": "Dhruv Patel<dhruv.codelab@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/dhruv-m-patel/ts-styled-react-app#readme",
  "dependencies": {
